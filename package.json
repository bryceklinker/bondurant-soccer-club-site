    "concurrently": "6.4.0",
    "cross-env": "7.0.3",
    "cypress": "8.7.0",
    "faker": "5.5.3",
    "glob": "7.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "react-hot-loader": "4.13.0",
    "react-static-plugin-typescript": "7.6.2",
    "serve": "13.0.2",
