    "axios": "0.27.2",
    "date-fns": "2.29.3",
    "next": "12.3.0",
    "react": "18.2.0",
    "react-collapsed": "3.4.0",
    "react-dom": "18.2.0",
    "react-transition-group": "4.4.5"
  },
  "devDependencies": {
    "@faker-js/faker": "7.5.0",
    "@testing-library/cypress": "8.0.3",
    "@testing-library/dom": "8.18.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
